<div class="bg-light fixed-bottom" id="trainer-footer">
  <div class="container">
    <div class="row">
      <div class="col">
        <%= link_to users_url, class: "footer-link" do %>
          <%= image_tag("search.svg", width: "18", height: "18") %>
          <p class="mt-1 mb-1">Search</p>
        <% end %>
      </div>
      <div class="col">
        <%= link_to user_my_trainers_url(current_user), class: "footer-link" do %>
          <%= image_tag("speed.svg", width: "20", height: "20") %>
          <p class="mt-1 mb-1">Trainrs</p>
        <% end %>
      </div>
      <div class="col">
        <%= link_to new_event_url, class: "footer-link" do %>
          <%= image_tag("plus.svg", width: "18", height: "18") %>
          <p class="mt-1 mb-1">Session</p>
        <% end %>
      </div>
      <div class="col">
        <%= link_to user_events_url(current_user), class: "footer-link" do %>
          <%= image_tag("calendar.svg", width: "18", height: "18") %>
          <p class="mt-1 mb-1">Schedule</p>
        <% end %>
      </div>
      <div class="col">
        <%= link_to user_url(current_user), class: "footer-link" do %>
          <%= image_tag("finger-print.svg", width: "18", height: "18") %>
          <p class="mt-1 mb-1">Trainr.bio</p>
        <% end %>
      </div>
    </div>
  </div>
</div>
